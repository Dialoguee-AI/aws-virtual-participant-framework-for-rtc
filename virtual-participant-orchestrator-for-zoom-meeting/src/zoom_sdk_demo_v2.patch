+			{
+				ZOOM_SDK_NAMESPACE::IAuthService* auth_service = SDKInterfaceWrap::GetInst().GetAuthService();
+				if (auth_service == NULL || auth_service->GetAuthResult() != ZOOM_SDK_NAMESPACE::AUTHRET_SUCCESS)
